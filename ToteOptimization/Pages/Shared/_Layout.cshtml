<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Tote Visualisation</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/lib/chartjs/Chart.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3" style="height:10%; width:100%; position:fixed; z-index:90">

            <div class="container" style= "margin-left: 0">
                
            <div id='cssmenu'>
                <ul>
                    <li>
                        <a id ="liveA" tabindex="1" asp-area="" asp-page="/LiveToteA"><span>4A</span></a>
                    </li>
                    <li>
                        <a id ="liveA" tabindex="2" asp-area="" asp-page="/LiveToteB"><span>4B</span></a>
                    </li>
                </ul>
            </div>
                    @*<button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                        <ul class="navbar-nav flex-grow-1"></ul>
                    </div>*@
                </div>
        </nav>
    </header>
    <div class="container" id="maincontain">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
